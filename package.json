{
  "private": false,
  "name": "racka",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "run-s webpack:dev tailwind",
    "preview": "run-s webpack:preview",
    "release": "run-s webpack:release tailwind",
    "webpack:dev": "webpack",
    "webpack:release": "webpack --mode production",
    "webpack:preview": "webpack-dev-server --mode development --open --hot",
    "tailwind": "tailwindcss -i tailwind.css -o ./dist/tailwind.css",
    "lint": "eslint src",
    "web-ext": "web-ext run",
    "dev": "run-s tailwind web-ext",
    "format:fix": "prettier --write ./src",
    "format": "prettier --check ./src"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "dist/"
    ]
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-preact": "^1.3.0",
    "preact-cli": "^3.4.2",
    "web-ext": "^7.6.2",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@preact/signals": "^1.1.3",
    "babel-loader": "^9.1.2",
    "html-webpack-plugin": "^5.5.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^3.2.1",
    "prettier": "^2.8.8",
    "react-reader": "^1.2.1",
    "striptags": "^3.2.0",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.3",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  }
}
